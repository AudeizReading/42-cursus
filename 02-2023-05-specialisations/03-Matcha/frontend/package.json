	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"start:prod": "NODE_ENV=production node dist/frontend/server/server.mjs",
		"start:ssl": "ng serve --host 0.0.0.0 --ssl",
		"build": "ng build --configuration development",
		"build:prod": "ng build --configuration production",
		"watch": "ng build --watch --configuration development",
		"test": "ng test --no-watch --code-coverage --no-progress --browsers=ChromeHeadless",
		"test:no-ci": "ng test --code-coverage --no-progress",
		"coverage": "open coverage/frontend/index.html",
		"lint": "ng lint",
		"lint:fix": "ng lint --fix",
		"prettier": "npx prettier --write .",
		"format": "npx prettier --write .",
		"serve:open": "ng serve --open",
		"serve:ssg": "npx serve dist/frontend/browser",
		"e2e": "ng e2e",
		"cypress:open": "cypress open",
		"cypress:run": "cypress run",
		"env": "cd ../ && ./env.conf.sh -e .env",
		"env:cp": "cd ../ && cp .env.example .env",
		"env:ask": "cd ../ && ./env.conf.sh",
		"env:dev": "cd ../ && ./env.conf.sh -e .env -d",
		"env:prod": "cd ../ && ./env.conf.sh -e .env -p",
		"env:help": "../env.conf.sh -h",
		"env:reset": "cd ../ && ./env.conf.sh -e .env -r",
		"proxy:config": "./proxy.conf.sh ../.env"
	},
	"private": true,
